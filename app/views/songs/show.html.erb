<h1><%= @song.title %></h1>
<h2>Artist: <%= @song.artist_name %></h2>
<h2>Genre: <%= @song.genre %></h2>

<% if(@song.released == true) %>
    <h2>Released: <%= @song.release_year %></h2>
<% end %>

<%= button_to "Edit", edit_song_path(@song),  method: :get, data: { confirm: "Are you sure?" } %><br>
<%= button_to "Delete", song_path(@song), method: :get, data: { confirm: "Are you sure?" } %><br>
<%= button_to "List of Songs", songs_path, method: :get %>